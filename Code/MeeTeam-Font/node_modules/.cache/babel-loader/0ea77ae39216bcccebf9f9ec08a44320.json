{"ast":null,"code":"import global_msg from '../utils/global.js';\nimport { login } from '@/api/login.js';\nexport default {\n  name: 'LogIn',\n\n  data() {\n    return {\n      user: '',\n      pass: '',\n      isLogin: false\n    };\n  },\n\n  methods: {\n    resetForm(formName) {\n      //重置表单\n      this.user = '';\n      this.pass = '';\n    },\n\n    //提交表单\n    submitForm(formName) {\n      if (this.user === \"\") {\n        alert(\"请输入用户名\");\n        return;\n      }\n\n      if (this.pass === \"\") {\n        alert(\"请输入密码\");\n        return;\n      } //请求地址,this和vm指的是全局\n\n\n      var vm = this;\n      let param = {\n        user: vm.user,\n        pass: vm.pass\n      };\n      login(param).then(function (res) {\n        if (res.data === false) {\n          vm.$message.error(\"登录失败\");\n          vm.resetForm(formName);\n        } else {\n          global_msg.nowuserid = vm.user; //改变全局nowuserid\n\n          vm.$message.success(\"登录成功\");\n          vm.$router.push({\n            path: \"/users/InforList\"\n          }); //接下来进入到哪个路由\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA;AACA;AACA;EACAA,aADA;;EAEAC;IACA;MACAC,QADA;MAEAC,QAFA;MAGAC;IAHA;EAKA,CARA;;EASAC;IACAC;MAAA;MACA;MACA;IACA,CAJA;;IAKA;IACAC;MACA;QACAC;QAEA;MACA;;MACA;QACAA;QACA;MACA,CATA,CAUA;;;MACA;MACA;QACAN,aADA;QAEAC;MAFA;MAIAM;QACA;UACAC;UACAA;QACA,CAHA,MAGA;UACAC,+BADA,CACA;;UACAD;UACAA;YAAAE;UAAA,GAHA,CAGA;QACA;MACA,CATA;IAUA;;EAhCA;AATA","names":["name","data","user","pass","isLogin","methods","resetForm","submitForm","alert","login","vm","global_msg","path"],"sourceRoot":"src/components","sources":["Login.vue"],"sourcesContent":["<template>\r\n   <div id=\"login-container\">\r\n    <div style=\"text-align: center;height: 50px\">\r\n      登录\r\n    </div>\r\n    <el-form :model=\"ruleForm\" status-icon :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n      <el-form-item label=\"账号\" prop=\"user\">\r\n        <el-input type=\"text\" v-model=\"user\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"密码\" prop=\"pass\">\r\n        <el-input type=\"password\" v-model=\"pass\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">提交</el-button>\r\n        <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport global_msg from '../utils/global.js'\r\nimport {login} from '@/api/login.js'\r\nexport default {\r\n   name: 'LogIn',\r\n   data() {\r\n    return {\r\n      user: '',\r\n      pass: '',\r\n      isLogin:false\r\n    };\r\n  },\r\n  methods: {\r\n    resetForm(formName) {//重置表单\r\n      this.user=''\r\n      this.pass=''\r\n    },\r\n     //提交表单\r\n    submitForm(formName) {\r\n      if(this.user===\"\"){\r\n      alert(\"请输入用户名\")\r\n    \r\n      return;\r\n      }\r\n      if(this.pass===\"\"){\r\n        alert(\"请输入密码\")\r\n        return;\r\n      }\r\n      //请求地址,this和vm指的是全局\r\n      var vm = this;\r\n      let param={\r\n        user:vm.user,\r\n        pass:vm.pass\r\n      }\r\n      login(param).then(function(res){\r\n        if(res.data===false){\r\n          vm.$message.error(\"登录失败\");\r\n          vm.resetForm(formName);\r\n        }else{\r\n          global_msg.nowuserid=vm.user;//改变全局nowuserid\r\n          vm.$message.success(\"登录成功\");\r\n          vm.$router.push({path: \"/users/InforList\"}); //接下来进入到哪个路由\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n body{\r\n    margin: 0;\r\n  }\r\n  #login-container{\r\n    width: 400px;\r\n    height: 290px;\r\n    background: #e5e9f2;\r\n    position: absolute;\r\n    left: 50%;\r\n    top: 50%;\r\n    margin-left: -220px;\r\n    margin-top: -170px;\r\n    border-radius: 5px;\r\n    padding-top: 40px;\r\n    padding-right: 40px;\r\n  }\r\n</style>"]},"me